{
  "name": "statisfy",
  "version": "1.17",
  "description": "Get statistics from all of your favourite social media sites, games and more!",
  "main": "./dist/index.js",
  "scripts": {
    "test": "node ./test/index.js",
    "build": "babel src --out-dir dist",
    "format": "eslint ./src/ --fix",
    "docs":"jsdoc ./jsdoc.conf.json -d ./docs -r --verbose -P ./package.json ./src",
    "start": "npm run docs && npm run build && npm link && npm link statisfy && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yarn-Development/statisfynpm.git"
  },
  "keywords": [
    "stats",
    "statistics",
    "social",
    "media",
    "api"
  ],
  "author": "Aspekts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yarn-Development/statisfynpm/issues"
  },
  "publishConfig":{
    "registry": "https://npm.pkg.github.io"
  },
  "homepage": "https://statisfy.js.org",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "ink-docstrap": "^1.3.2",
    "node-fetch": "^2.6.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.16.3",
    "eslint": "^8.14.0"
  }
}
